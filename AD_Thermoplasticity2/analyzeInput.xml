<ParameterList name="Problem">
  <Parameter name="Physics" type="string" value="Plato Driver"/>
  <Parameter  name="Spatial Dimension"    type="int"    value="3"/>

  <Parameter name="Input Mesh" type="string" value="pin.exo"/>
  
  <ParameterList name="Plato Problem">
    <Parameter name="Physics"            type="string"  value="Thermoplasticity"/>
    <Parameter name="PDE Constraint"     type="string"  value="Elliptic"/>

    <ParameterList name='Criteria'>                                                    
      <ParameterList name='Objective'>                                                               
        <Parameter name='Type' type='string' value='Weighted Sum'/>                                  
        <Parameter name='Functions' type='Array(string)' value='{My Total Work,My Thermal Energy}'/> 
        <Parameter name='Weights' type='Array(double)' value='{1.0e0,0.0e-5}'/>                         
      </ParameterList>                                                                               
      <ParameterList name='My Total Work'>                                                         
        <Parameter name='Type'                 type='string' value='Scalar Function'/>               
        <Parameter name='Scalar Function Type' type='string' value='Elastic Work'/>                  
        <Parameter name='Exponent'             type='double' value='3.0'/>                           
        <Parameter name='Minimum Value'        type='double' value='1.0e-8'/>                        
      </ParameterList>                                                                               
      <ParameterList name='My Thermal Energy'>                                                         
        <Parameter name='Type'                 type='string' value='Scalar Function'/>               
        <Parameter name='Scalar Function Type' type='string' value='Thermal Energy'/>                  
        <Parameter name='Exponent'             type='double' value='1.0'/>                           
        <Parameter name='Minimum Value'        type='double' value='1.0e-8'/>                        
      </ParameterList>                                                                                                                                              
    </ParameterList>

    <ParameterList name='Linear Solver'>                                               
      <Parameter name='Solver Stack' type='string' value='AmgX'/>
    </ParameterList>

    <ParameterList name='Time Stepping'>                                               
      <Parameter name='Initial Num. Pseudo Time Steps' type='int' value='8'/>          
      <Parameter name='Maximum Num. Pseudo Time Steps' type='int' value='8'/>          
    </ParameterList>

    <ParameterList name='Newton-Raphson'>                                              
      <Parameter name='Stop Measure' type='string' value='residual'/>                  
      <Parameter name='Maximum Number Iterations' type='int' value='40'/> 
      <Parameter name='Stopping Tolerance' type='double' value='1e-8'/> 
      <Parameter name='Current Residual Norm Stopping Tolerance' type='double' value='1e-8'/>   
    </ParameterList>                                                                   

    <ParameterList name="Elliptic">
      <ParameterList name="Penalty Function">
        <Parameter name="Type" type="string" value="SIMP"/>
        <Parameter name="Exponent" type="double" value="3.0"/>
        <Parameter name="Minimum Value" type="double" value="1.0e-8"/>
      </ParameterList>
      <Parameter  name='Plottable' type='Array(string)' value='{deviatoric stress,plastic multiplier increment,accumulated plastic strain}'/>
    </ParameterList>

    <ParameterList name="Spatial Model">
      <ParameterList name="Domains">
        <ParameterList name="Design Volume">
          <Parameter name="Element Block" type="string" value="block_1"/>
          <Parameter name="Material Model" type="string" value="Unobtainium"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Material Models">
      <Parameter  name='Pressure Scaling' type='double' value='100.0'/>
      <Parameter  name='Temperature Scaling' type='double' value='100.0'/>
      <ParameterList name="Unobtainium">
        <ParameterList name='Isotropic Linear Thermoelastic'>
          <Parameter name='Mass Density' type='double' value='8e-9'/>
          <Parameter  name='Poissons Ratio' type='double' value='0.3'/>
          <Parameter  name='Youngs Modulus' type='double' value='75.0e3'/>
          <Parameter  name='Thermal Conductivity' type='double' value='16.0'/>
          <Parameter  name='Thermal Expansivity' type='double' value='2.32e-5'/>
          <Parameter  name='Reference Temperature' type='double' value='100.0'/>
        </ParameterList>
        <ParameterList name='Plasticity Model'>                                             
          <ParameterList name='J2 Plasticity'>                                                
            <Parameter  name='Hardening Modulus Isotropic' type='double' value='1.0e3'/>      
            <Parameter  name='Hardening Modulus Kinematic' type='double' value='1.0e3'/>      
            <Parameter  name='Initial Yield Stress' type='double' value='344.0'/>             
            <Parameter  name='Elastic Properties Penalty Exponent' type='double' value='3.0'/>  
            <Parameter  name='Elastic Properties Minimum Ersatz' type='double' value='1e-8'/> 
            <Parameter  name='Plastic Properties Penalty Exponent' type='double' value='2.5'/>
            <Parameter  name='Plastic Properties Minimum Ersatz' type='double' value='1e-4'/> 
          </ParameterList>                                                                    
        </ParameterList>                                                                      
      </ParameterList>
    </ParameterList>
  
    <ParameterList  name="Natural Boundary Conditions">
      <ParameterList  name="Mechanical Natural Boundary Conditions">
        <ParameterList  name="Traction Vector Boundary Condition">
          <Parameter  name="Type"     type="string"        value="Uniform"/>
          <Parameter  name="Values"   type="Array(string)" value="{0.0, -5.0*t, 0.0}"/>
          <Parameter  name="Sides"    type="string"        value="traction"/>
        </ParameterList>
      </ParameterList>
      <ParameterList  name="Thermal Natural Boundary Conditions">
        <ParameterList  name="Surface Flux Boundary Condition">
          <Parameter  name="Type"     type="string"   value="Uniform"/>
          <Parameter  name="Value"    type="string"   value="50.0*t"/>
          <Parameter  name="Sides"    type="string"   value="flux"/>
        </ParameterList>
      </ParameterList>                                                          
    </ParameterList>
    
    <ParameterList  name='Essential Boundary Conditions'> 
      <ParameterList  name='X Fixed Displacement Boundary Condition'>        
        <Parameter  name='Type'     type='string' value='Zero Value'/>       
        <Parameter  name='Index'    type='int'    value='0'/>                
        <Parameter  name='Sides'    type='string' value='fixed_xy'/>            
      </ParameterList>                                                       
      <ParameterList  name='Y Fixed Displacement Boundary Condition'>        
        <Parameter  name='Type'     type='string' value='Zero Value'/>       
        <Parameter  name='Index'    type='int'    value='1'/>                
        <Parameter  name='Sides'    type='string' value='fixed_xy'/>            
      </ParameterList>
      <ParameterList  name='Z Fixed Displacement Boundary Condition'>        
        <Parameter  name='Type'     type='string' value='Zero Value'/>       
        <Parameter  name='Index'    type='int'    value='2'/>                
        <Parameter  name='Sides'    type='string' value='fixed_z'/>            
      </ParameterList>
      <ParameterList  name='Temperature Fixed 1 Boundary Condition'>        
        <Parameter  name='Type'     type='string' value='Time Dependent'/>       
        <Parameter  name='Index'    type='int'    value='3'/>                
        <Parameter  name='Sides'    type='string' value='fixed_xy'/>
        <Parameter  name='Function' type='string' value='100'/>           
      </ParameterList>                                         
    </ParameterList>    

  </ParameterList>

</ParameterList>
