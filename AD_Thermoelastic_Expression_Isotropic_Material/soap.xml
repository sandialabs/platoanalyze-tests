<ParameterList name="Problem">
  <Parameter name="Physics" type="string" value="Plato Driver"/>

  <Parameter name="Input Mesh" type="string" value="./soap.exo"/>

  <ParameterList name="Plato Problem">
    <Parameter name="Physics"         type="string"  value="Thermomechanical"/>
    <Parameter name="PDE Constraint"  type="string"  value="Elliptic"/>
    <ParameterList name="Linear Solver">
       <Parameter name="Solver Stack"  type="string"  value="AmgX"/>
    </ParameterList>

    <Parameter name="Objective"       type="string"  value="My Internal Elastic Energy"/>

    <Parameter name="Self-Adjoint"       type="bool"    value="true"/>

    <ParameterList name="My Internal Elastic Energy">
      <Parameter name='Type' type='string' value='Scalar Function'/> 
      <Parameter name='Scalar Function Type' type='string' value='Internal Elastic Energy'/>
      <ParameterList name="Penalty Function">
        <Parameter name="Type" type="string" value="SIMP"/>
        <Parameter name="Exponent" type="double" value="1.0"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Elliptic">
      <Parameter  name="Plottable"   type="Array(string)" value="{strain,stress}"/>
      <ParameterList name="Penalty Function">
        <Parameter name="Type" type="string" value="SIMP"/>
        <Parameter name="Exponent" type="double" value="1.0"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Spatial Model">
      <ParameterList name="Domains">
        <ParameterList name="Design Volume">
          <Parameter name="Element Block" type="string" value="block_1"/>
          <Parameter name="Material Model" type="string" value="Unobtainium"/>
        </ParameterList>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Material Models">
      <ParameterList name="Unobtainium">
        <ParameterList name="Thermoelastic">
          <Parameter  name="Thermal Expansivity" type="double" value="0.0"/>
          <Parameter  name="Thermal Conductivity" type="double" value="910.0"/>
          <Parameter  name="Reference Temperature" type="double" value="10.0"/>
          <ParameterList name='Elastic Stiffness Expression'>
            <Parameter name='symmetry' type='string' value='isotropic' />
            <ParameterList name='Youngs Modulus'>
              <Parameter name='Constant Names' type='Array(string)' value='{E0}'/>
              <Parameter name='Constant Values' type='Array(double)' value='{1e8}'/>
              <Parameter name='Independent Variable Name' type='string' value='Z'/>
              <Parameter name='Expression' type='string' value='E0*Z'/>
            </ParameterList>
            <ParameterList name='Poissons Ratio'>
              <Parameter name='Constant Names' type='Array(string)' value='{v0}'/>
              <Parameter name='Constant Values' type='Array(double)' value='{0.3}'/>
              <Parameter name='Independent Variable Name' type='string' value='Z'/>
              <Parameter name='Expression' type='string' value='v0*Z'/>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
  
    <ParameterList  name="Mechanical Natural Boundary Conditions">
      <ParameterList  name="Traction Vector Boundary Condition">
        <Parameter  name="Type"     type="string"        value="Uniform"/>
        <Parameter  name="Values"   type="Array(double)" value="{1.0, 0.0, 0.0}"/>
        <Parameter  name="Sides"    type="string"        value="ss_2"/>
      </ParameterList>
    </ParameterList>

    <ParameterList  name="Essential Boundary Conditions">
      <ParameterList  name="X Fixed Displacement Boundary Condition">
        <Parameter  name="Type"     type="string" value="Zero Value"/>
        <Parameter  name="Index"    type="int"    value="0"/>
        <Parameter  name="Sides"    type="string" value="ns_1440"/>
      </ParameterList>
      <ParameterList  name="Y Fixed Displacement Boundary Condition">
        <Parameter  name="Type"     type="string" value="Zero Value"/>
        <Parameter  name="Index"    type="int"    value="1"/>
        <Parameter  name="Sides"    type="string" value="ns_1440"/>
      </ParameterList>
      <ParameterList  name="Z Fixed Displacement Boundary Condition">
        <Parameter  name="Type"     type="string" value="Zero Value"/>
        <Parameter  name="Index"    type="int"    value="2"/>
        <Parameter  name="Sides"    type="string" value="ns_1440"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

</ParameterList>

